    "import math\n",
    "\n",
    "points = [\n",
    "    (2, -2),\n",
    "    (5, 6),\n",
    "    (-4, -4),\n",
    "    (-7, 1),\n",
    "    (8, 14)\n",
    "]\n",
    "\n",
    "m = 1.2\n",
    "b = 2\n",
    "\n",
    "errors = []\n",
    "\n",
    "for p in points:\n",
    "    errors.append(abs(p[1] - (m*p[0]+b)))\n",
    "\n",
    "print(sum(errors)/len(errors))"
   ]
  }
